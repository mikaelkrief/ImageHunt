{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Admin": {
    "Name": "ADMIN_USER",
    "Email": "ADMIN_EMAIL",
    "Password": "ADMIN_PASSWORD"
  },
  "Jwt": {
    "Key": "GOOGLEAPI_CLIENT_SECRET",
    "Issuer": "http://imagehunt.mandrillon.fr",
    "ExpireDays": 30

  },

  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=ImageHunt;Uid=HuntUser;Pwd=DBPASSWORD;"
  },
  "GoogleApi": {
    "ClientId": "GOOGLEAPI_CLIENT_ID",
    "ClientSecret": "GOOGLEAPI_CLIENT_SECRET"
  },
  "ImageHuntApi": {
    "Url": "http://localhost:43454"
  },
  "BotConfiguration": {
    "BotName": "BOT_NAME",
    "BotToken": "TELEGRAM_APIKEY",
    "StorageFolder": "BOT_STORAGE_FOLDER",
    "BotUrl": "BOT_URL",
    "BotEmail": "BOT_EMAIL",
    "BotPassword": "BOT_PASSWORD"
  }
}
