{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    },
    "LogDirectory": "/var/log/"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=ImageHunt;Uid=HuntUser;Pwd=DBPASSWORD;"
  },
  "GoogleApi": {
    "ClientId": "GOOGLEAPI_CLIENT_ID",
    "ClientSecret": "GOOGLEAPI_CLIENT_SECRET"
  },
  "Telegram": {
    "APIKey": "TELEGRAM_APIKEY"
  },
  "ImageHuntApi": {
    "Url": "http://localhost:43454",
    "AuthToken": "ImageHuntBot ImageHuntBotToken"
  },
  "BotConfiguration": {
    "BotName": "BOT_NAME",
    "BotToken": "TELEGRAM_APIKEY",
    "StorageFolder": "/var/cache/ImageHuntBot/",
    "PathToCertificate": "/etc/nginx/certificates/ImageHuntBot.pem",
    "BotUrl": "https://imagehuntbot.mandrillon.fr",
    "WebhookUrl": "https://imagehuntbot.mandrillon.fr"
  }


}
